@model SchoolSystem.ViewModels.FeedbackViewModel

@{
    ViewData["Title"] = "EditFeedback";
}

<h1>Edit Feedback</h1>
<div>
    <a asp-action="ViewFeedbacks">Back to List</a>
</div>
<hr />
<div class="row justify-content-center">
    <div class="col-md-4 p-5 bg-white mt-5 shadow-lg">
        <form asp-action="EditFeedback">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="FeedbackTxt" class="control-label"></label>
                <input asp-for="FeedbackTxt" class="form-control" />
                <span asp-validation-for="FeedbackTxt" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>



